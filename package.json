{
  "name": "logger",
  "version": "1.0.0",
  "description": "Qare logger for Node.JS",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/babel src --source-maps --out-dir dist",
    "eslint": "node_modules/eslint/bin/eslint.js --max-warnings=0 src",
    "flow": "node_modules/.bin/flow",
    "test:unit": "mocha --compilers js:babel-core/register test/*",
    "test": "yarn eslint && yarn flow && yarn test:unit",
    "postinstall": "yarn build",
    "prettier": "prettier --config ./.prettierrc --write '{src,test}/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qare/logger.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qare/logger/issues"
  },
  "homepage": "https://github.com/qare/logger#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-promise-to-bluebird": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-qare": "git+https://github.com/qare/eslint-config.git",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "flow-bin": "^0.104.0",
    "husky": "^3.0.3",
    "log4js": "^4.5.1"
  }
}
